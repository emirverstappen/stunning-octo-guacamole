#include <iostream>
using namespace std;
int dogrusalArama(int dizi[], int aranan) {
	int i;
	for (i = 0; i < 10; i++)
	{
		if (dizi[i]==aranan)
		{
			return i;
		}
	}
	return -1;
}
int main()
{
	int aranan;
	int dizi[10] = { 6,3,1,4,2,9,7,8,5,0 };
	cout << "aranan degeri girin: ";
	cin >> aranan;
	cout<<dogrusalArama(dizi, aranan);
}
